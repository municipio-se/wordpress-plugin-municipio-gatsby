{
  "name": "municipio/municipio-gatsby",
  "description": "Adapts Municipio for Gatsby",
  "type": "wordpress-plugin",
  "license": "AGPL-3.0-or-later",
  "autoload": {
    "psr-4": {
      "WPMunicipioGatsby\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Whitespace",
      "email": "code@whitespace.se"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "require": {
    "jawira/case-converter": "^3.4",
    "symfony/yaml": "^5.4"
  },
  "require-dev": {
    "wp-graphql/wp-graphql": "^1.3",
    "wp-graphql/wp-graphql-acf": "^0.5.1",
    "helsingborg-stad/modularity": "^2.13",
    "wpackagist-plugin/advanced-custom-fields": "^5.9"
  },
  "config": {
    "allow-plugins": {
      "composer/installers": true
    }
  }
}
